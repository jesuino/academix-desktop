<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="EnderecoController">
  <center>
    <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
      <columns>
        <TableColumn prefWidth="75.0" text="CEP" fx:id="cep" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="248.0" text="Logradouro" fx:id="logradouro" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="188.0" text="Bairro" fx:id="bairro" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="172.0" text="Cidade" fx:id="cidade" />
        <TableColumn prefWidth="75.0" text="Estado" fx:id="estado" />
      </columns>
    </TableView>
  </center>
  <top>
    <MenuBar>
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="Close" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Cadastros">
          <items>
            <MenuItem mnemonicParsing="false" text="Enderecos" />
            <MenuItem mnemonicParsing="false" text="Campus" />
            <MenuItem mnemonicParsing="false" text="Curso" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
  </top>
</BorderPane>
